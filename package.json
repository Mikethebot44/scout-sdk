{
  "name": "scout-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for RAG-enhanced AI applications with vector search and content indexing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "npm run build && node --test dist/**/*.test.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "sdk",
    "rag",
    "vector-search",
    "ai",
    "typescript",
    "pinecone",
    "openai",
    "github",
    "documentation",
    "embeddings",
    "chatbot",
    "context"
  ],
  "author": "Terragon Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/terragon-labs/scout-sdk"
  },
  "bugs": {
    "url": "https://github.com/terragon-labs/scout-sdk/issues"
  },
  "homepage": "https://github.com/terragon-labs/scout-sdk#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@pinecone-database/pinecone": "^3.0.3",
    "@mozilla/readability": "^0.5.0",
    "@octokit/rest": "^21.0.2",
    "jsdom": "^25.0.1",
    "openai": "^4.67.3",
    "playwright": "^1.48.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "express": "^4.18.0",
    "next": "^14.0.0 || ^15.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}